<Window x:Class="Beauty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="唐氏筛查" Height="700" Width="1024" WindowState="Normal" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize"  Icon="/Beauty;component/Image/1.png" >
    <Viewbox>
    <Canvas x:Name="myCanvas" Height="666" Width="1024">
        <Canvas x:Name="myToolBar" Height="25" Width="1024" Canvas.Left="0" Canvas.Top="0">
            <Menu Canvas.Left="0" Canvas.Top="2" Height="23" Name="Menus" Width="1024">
                <MenuItem Header="工作台" x:Name="Inputs"></MenuItem>
                <MenuItem Header="默认值" x:Name="UserSetting"></MenuItem>
                <MenuItem Header="接口对照" x:Name="InterFaceSetting" Visibility="Collapsed"></MenuItem>
                <MenuItem Header="帮助" >
                    <MenuItem Header="注册" x:Name="RegisterSoftware"/>
                    <MenuItem Header="关于" x:Name="AboutMe"/>
                </MenuItem>
                <MenuItem Header="退出" x:Name="Exit"></MenuItem>
            </Menu>

        </Canvas>
        <Canvas x:Name="myTitle" Height="70" Width="1024" Canvas.Left="0" Canvas.Top="25"></Canvas>
        <Canvas x:Name="myContent" Height="565" Width="1024" Canvas.Left="0" Canvas.Top="95"></Canvas>
        
    </Canvas>
    </Viewbox>
</Window>
