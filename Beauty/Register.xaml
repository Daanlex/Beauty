        <TextBox Canvas.Left="135" Canvas.Top="70" Height="23" Name="tbRegistCode" Width="130" />
        <Button Canvas.Left="93" Canvas.Top="138" Content="注册" Height="23" Name="btnRegist" Width="75" />
        <TextBlock Canvas.Left="24" Canvas.Top="193" Height="34" Name="registeredState" Visibility="Collapsed" Text="您已经注册完毕"  Width="221" FontSize="20" Foreground="#FFF20909" />
    </Canvas>
