            <TextBox Canvas.Left="281" Canvas.Top="188" Height="23" Name="tbBadNo" Text="" Width="93" />
            <TextBlock Canvas.Left="189" Canvas.Top="216" Height="23" Name="textBlock32" Text="电话" Width="86" />
            <TextBox Canvas.Left="281" Canvas.Top="216" Height="23" Name="tbPatientTel" Text="" Width="93" />
            <TextBlock Canvas.Left="189" Canvas.Top="244" Height="23" Name="textBlock33" Text="家庭地址" Width="86" />
            <TextBox Canvas.Left="281" Canvas.Top="244" Height="23" Name="tbPatientAddress" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="19" Height="23" Name="textBlock8" Text="实验编号" Width="86" />
            <TextBox  Canvas.Left="516" Canvas.Top="19" Height="23" Name="tbSpecimenNo" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="47" Height="23" Name="textBlock11" Text="采样日期" Width="86" />
            <TextBox  Canvas.Left="516" Canvas.Top="47" Height="23" Name="tbCollectionDate" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="74" Height="23" Name="textBlock12" Text="体重" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="74" Height="23" Name="tbWGHT" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="102" Height="23" Name="textBlock14" Text="胎儿数量" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="102" Height="23" Name="tbFETU" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="132" Height="23" Name="textBlock19" Text="AFP" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="129" Height="23" Name="tbAFP" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="160" Height="23" Name="textBlock20" Text="HCG" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="157" Height="23" Name="tbHCG" Text="" Width="93" />
