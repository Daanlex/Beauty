            <TextBox Canvas.Left="516" Canvas.Top="185" Height="23" Name="tbUE3" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="216" Height="23" Name="textBlock36" Text="孕周" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="213" Height="23" Name="tbGestationalWeek" Text="" Width="93" />
            <TextBlock Canvas.Left="405" Canvas.Top="244" Height="23" Name="textBlock37" Text="B超孕周" Width="86" />
            <TextBox Canvas.Left="516" Canvas.Top="241" Height="23" Name="tbGestationalWeekByBC" Text="" Width="93" />
        </Canvas>
    </Canvas>
</UserControl>
