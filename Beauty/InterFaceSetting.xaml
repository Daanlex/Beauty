            <TextBlock Canvas.Left="189" Canvas.Top="160" Height="23" Name="textBlock40" Text="住院号" Width="86" />
            <TextBox Canvas.Left="281" Canvas.Top="160" Height="23" Name="tbHospitalizedNo" Text="" Width="93" />
